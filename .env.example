# Neo4j Database Configuration
# Format: neo4j://hostname:port
NEO4J_URI=neo4j://localhost:7687
# Default username is neo4j unless changed during setup
NEO4J_USER=neo4j
# Password set during Neo4j installation
NEO4J_PASSWORD=your_password_here

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Model Configuration
# Default model to use for OpenAI (e.g., gpt-3.5-turbo, gpt-4)
OPENAI_MODEL=gpt-3.5-turbo
# Default embedding model for sentence transformers
SENTENCE_TRANSFORMER_MODEL=all-MiniLM-L6-v2 