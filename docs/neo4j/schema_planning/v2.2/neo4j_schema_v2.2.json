{
  "version": "2.2",
  "last_updated": "2024-02",
  "nodes": {
    "AICategory": {
      "description": "Core AI technology categories",
      "properties": {
        "id": {
          "type": "uuid",
          "required": true,
          "indexed": true,
          "description": "Unique identifier"
        },
        "name": {
          "type": "string",
          "required": true,
          "unique": true,
          "indexed": true,
          "description": "Category name"
        },
        "category_definition": {
          "type": "text",
          "required": true,
          "description": "Detailed category description"
        },
        "status": {
          "type": "enum",
          "values": ["Active", "Deprecated", "Proposed"],
          "indexed": true
        },
        "maturity_level": {
          "type": "enum",
          "values": ["Emerging", "Established", "Mature"],
          "indexed": true
        },
        "zone_id": {
          "type": "uuid",
          "references": "Zone.id",
          "description": "Reference to technical zone"
        },
        "created_at": {
          "type": "datetime",
          "description": "Creation timestamp"
        },
        "last_updated": {
          "type": "datetime",
          "description": "Last modification timestamp"
        },
        "version": {
          "type": "string",
          "pattern": "^\\d+\\.\\d+\\.\\d+$",
          "description": "Semantic version number"
        }
      }
    },
    "Zone": {
      "description": "Technical zones",
      "properties": {
        "id": {
          "type": "uuid",
          "required": true,
          "indexed": true,
          "description": "Unique identifier"
        },
        "name": {
          "type": "string",
          "required": true,
          "unique": true,
          "indexed": true,
          "description": "Zone name"
        },
        "description": {
          "type": "string",
          "description": "Zone description"
        },
        "created_at": {
          "type": "datetime",
          "description": "Creation timestamp"
        },
        "last_updated": {
          "type": "datetime",
          "description": "Last modification timestamp"
        }
      }
    },
    "Keyword": {
      "description": "Keywords for AI technology categorization",
      "properties": {
        "id": {
          "type": "uuid",
          "required": true,
          "indexed": true,
          "description": "Unique identifier"
        },
        "name": {
          "type": "string",
          "required": true,
          "unique": true,
          "indexed": true,
          "description": "Keyword name"
        },
        "type": {
          "type": "string",
          "description": "Type of keyword"
        },
        "relevance_score": {
          "type": "float",
          "description": "Base relevance score"
        },
        "created_at": {
          "type": "datetime",
          "description": "Creation timestamp"
        },
        "last_updated": {
          "type": "datetime",
          "description": "Last modification timestamp"
        }
      }
    },
    "UseCase": {
      "description": "Federal AI use cases",
      "properties": {
        "id": {
          "type": "uuid",
          "required": true,
          "indexed": true
        },
        "name": {
          "type": "string",
          "required": true,
          "indexed": true,
          "description": "Use case name"
        },
        "topic_area": {
          "type": "enum",
          "indexed": true
        },
        "stage": {
          "type": "enum",
          "indexed": true
        },
        "impact_type": {
          "type": "enum",
          "indexed": true
        },
        "purpose_benefits": {
          "type": "text",
          "description": "Intended purpose and benefits"
        },
        "outputs": {
          "type": "text",
          "description": "System outputs description"
        },
        "dev_method": {
          "type": "enum",
          "description": "Development method"
        },
        "contains_pii": {
          "type": "boolean",
          "indexed": true,
          "nullable": true
        },
        "has_ato": {
          "type": "boolean",
          "indexed": true,
          "nullable": true
        },
        "system_name": {
          "type": "string",
          "nullable": true
        },
        "date_initiated": {
          "type": "datetime",
          "nullable": true
        },
        "date_acquisition": {
          "type": "datetime",
          "nullable": true
        },
        "date_implemented": {
          "type": "datetime",
          "nullable": true
        },
        "date_retired": {
          "type": "datetime",
          "nullable": true
        },
        "created_at": {
          "type": "datetime"
        },
        "last_updated": {
          "type": "datetime"
        }
      }
    },
    "Agency": {
      "description": "Federal agencies",
      "properties": {
        "id": {
          "type": "uuid",
          "required": true,
          "indexed": true
        },
        "name": {
          "type": "string",
          "required": true,
          "unique": true,
          "indexed": true
        },
        "abbreviation": {
          "type": "string",
          "required": true,
          "unique": true,
          "indexed": true
        },
        "created_at": {
          "type": "datetime"
        },
        "last_updated": {
          "type": "datetime"
        }
      }
    },
    "Bureau": {
      "description": "Agency bureaus/offices",
      "properties": {
        "id": {
          "type": "uuid",
          "required": true,
          "indexed": true
        },
        "name": {
          "type": "string",
          "required": true,
          "indexed": true
        },
        "agency_id": {
          "type": "uuid",
          "required": true,
          "references": "Agency.id",
          "indexed": true
        },
        "created_at": {
          "type": "datetime"
        },
        "last_updated": {
          "type": "datetime"
        }
      }
    },
    "AIClassification": {
      "properties": {
        "id": {"type": "uuid", "required": true},
        "match_type": {
          "type": "enum",
          "required": true,
          "values": ["PRIMARY", "SUPPORTING", "RELATED"]
        },
        "match_rank": {"type": "integer"},
        "confidence": {
          "type": "float",
          "required": true,
          "min": 0.0,
          "max": 1.0
        },
        "analysis_method": {
          "type": "enum",
          "required": true,
          "values": ["KEYWORD", "SEMANTIC", "LLM", "ENSEMBLE"]
        },
        "analysis_version": {"type": "string"},
        "keyword_score": {"type": "float"},
        "semantic_score": {"type": "float"},
        "llm_score": {"type": "float"},
        "field_match_scores": {"type": "json"},
        "term_match_details": {"type": "json"},
        "matched_keywords": {"type": "string[]"},
        "llm_verification": {"type": "boolean"},
        "llm_confidence": {"type": "float"},
        "llm_reasoning": {"type": "string"},
        "llm_suggestions": {"type": "json"},
        "improvement_notes": {"type": "string[]"},
        "false_positive": {"type": "boolean"},
        "manual_override": {"type": "boolean"},
        "review_status": {
          "type": "enum",
          "values": ["PENDING", "REVIEWED", "VERIFIED"]
        },
        "classified_at": {"type": "datetime"},
        "classified_by": {"type": "string"},
        "last_updated": {"type": "datetime"}
      }
    },
    "NoMatchAnalysis": {
      "properties": {
        "id": {"type": "uuid", "required": true},
        "reason": {"type": "string", "required": true},
        "confidence": {"type": "float"},
        "llm_analysis": {"type": "json"},
        "suggested_keywords": {"type": "string[]"},
        "improvement_suggestions": {"type": "json"},
        "created_at": {"type": "datetime"},
        "analyzed_by": {"type": "string"},
        "status": {
          "type": "enum",
          "values": ["NEW", "REVIEWED", "ACTIONED"]
        },
        "review_notes": {"type": "string"}
      }
    }
  },
  "relationships": {
    "CLASSIFIES": {
      "source": "AICategory",
      "target": "AIClassification",
      "cardinality": "ONE_TO_MANY",
      "properties": {
        "created_at": {"type": "datetime"}
      }
    },
    "CLASSIFIED_AS": {
      "source": "UseCase",
      "target": "AIClassification",
      "cardinality": "ONE_TO_MANY",
      "properties": {
        "created_at": {"type": "datetime"}
      }
    },
    "HAS_ANALYSIS": {
      "source": "UseCase",
      "target": "NoMatchAnalysis",
      "cardinality": "ONE_TO_MANY",
      "properties": {
        "created_at": {"type": "datetime"}
      }
    },
    "SUGGESTS_CATEGORY": {
      "source": "NoMatchAnalysis",
      "target": "AICategory",
      "cardinality": "MANY_TO_MANY",
      "properties": {
        "created_at": {"type": "datetime"},
        "confidence": {"type": "float"}
      }
    },
    "HAS_BUREAU": {
      "source": "Agency",
      "target": "Bureau",
      "cardinality": "ONE_TO_MANY",
      "properties": {
        "created_at": {"type": "datetime"}
      }
    },
    "IMPLEMENTED_BY": {
      "source": "UseCase",
      "target": "Agency",
      "cardinality": "MANY_TO_ONE",
      "properties": {
        "created_at": {"type": "datetime"}
      }
    },
    "MANAGED_BY": {
      "source": "UseCase",
      "target": "Bureau",
      "cardinality": "MANY_TO_ONE",
      "properties": {
        "created_at": {"type": "datetime"}
      }
    },
    "BELONGS_TO": {
      "source": "AICategory",
      "target": "Zone",
      "cardinality": "MANY_TO_ONE",
      "properties": {
        "created_at": {"type": "datetime"},
        "weight": {"type": "float"}
      }
    },
    "HAS_KEYWORD": {
      "source": "AICategory",
      "target": "Keyword",
      "cardinality": "MANY_TO_MANY",
      "properties": {
        "relevance": {"type": "float"},
        "created_at": {"type": "datetime"}
      }
    },
    "DEPENDS_ON": {
      "source": "AICategory",
      "target": "AICategory",
      "cardinality": "MANY_TO_MANY",
      "properties": {
        "strength": {"type": "float"},
        "created_at": {"type": "datetime"}
      }
    }
  },
  "constraints": [
    {
      "type": "unique",
      "node": "UseCase",
      "property": "id"
    },
    {
      "type": "unique",
      "node": "AICategory",
      "property": "name"
    },
    {
      "type": "unique",
      "node": "Agency",
      "property": "name"
    },
    {
      "type": "unique",
      "node": "Agency",
      "property": "abbreviation"
    },
    {
      "type": "unique",
      "node": "Zone",
      "property": "name"
    },
    {
      "type": "unique",
      "node": "Keyword",
      "property": "name"
    },
    {
      "type": "reference",
      "source": "Bureau",
      "property": "agency_id",
      "target": "Agency.id"
    },
    {
      "type": "single",
      "node": "AIClassification",
      "property": "match_type",
      "where": "match_type = 'PRIMARY'"
    }
  ],
  "indexes": [
    {
      "type": "btree",
      "node": "UseCase",
      "property": "id"
    },
    {
      "type": "btree",
      "node": "AICategory",
      "property": "name"
    },
    {
      "type": "btree",
      "node": "Agency",
      "property": "name"
    },
    {
      "type": "btree",
      "node": "Agency",
      "property": "abbreviation"
    },
    {
      "type": "btree",
      "node": "Zone",
      "property": "name"
    },
    {
      "type": "btree",
      "node": "Keyword",
      "property": "name"
    },
    {
      "type": "btree",
      "node": "AIClassification",
      "property": "match_type"
    },
    {
      "type": "btree",
      "node": "NoMatchAnalysis",
      "property": "status"
    },
    {
      "type": "fulltext",
      "node": "UseCase",
      "properties": ["name", "description", "purpose_benefits"]
    },
    {
      "type": "fulltext",
      "node": "AICategory",
      "property": "definition"
    },
    {
      "type": "fulltext",
      "node": "Keyword",
      "property": "name"
    }
  ]
} 